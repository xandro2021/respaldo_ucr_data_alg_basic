package com.holamundo.mvc.models;

public class Persona {

	private int id;
	private String nombre;
	private int edad;
	private boolean vivo;

	public Persona(int id, String nombre, int edad, boolean vivo) {
		super();
		this.id = id;
		this.nombre = nombre;
		this.edad = edad;
		this.vivo = vivo;
	}


}
